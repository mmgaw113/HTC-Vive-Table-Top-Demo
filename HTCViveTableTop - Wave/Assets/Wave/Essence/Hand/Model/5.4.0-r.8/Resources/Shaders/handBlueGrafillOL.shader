				float2 texCoord10 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float clampResult20 = clamp( ( _Gradient_level + ( ( _Gradient_level - texCoord10.y ) / _Gradient_Blur ) ) , 0.0 , 1.0 );
				float4 lerpResult27 = lerp( _GraColorA , _GraColorB , clampResult20);
				
				float4 color25 = IsGammaSpace() ? float4(0,0,0,0) : float4(0,0,0,0);
				float4 color21 = IsGammaSpace() ? float4(1,1,1,0) : float4(1,1,1,0);
				float smoothstepResult26 = smoothstep( -0.05 , 1.0 , texCoord10.y);
				float4 lerpResult29 = lerp( color25 , color21 , smoothstepResult26);
				float2 texCoord17 = IN.ase_texcoord3.zw * float2( 1,1 ) + float2( 0,0 );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = lerpResult27.rgb;
				float Alpha = ( lerpResult29 * _Opacity * tex2D( _AlphaText, texCoord17 ) ).r;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

